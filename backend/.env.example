# Environment variables for the RAG chatbot
# Copy this to .env and fill in your values

# OpenAI API key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Qdrant Cloud (free tier at cloud.qdrant.io)
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key_here

# Neon Postgres (optional, for user data)
DATABASE_URL=postgresql://user:pass@endpoint.neon.tech/dbname

# Debug mode
DEBUG=true
